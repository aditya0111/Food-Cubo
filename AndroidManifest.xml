<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.foodcuboprovider"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />
     <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> 
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.foodcuboprovider.MainActivity"
            android:label="@string/app_name" >
        </activity>
          <activity android:name="Splash_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service
          android:name="com.amazonaws.mobileconnectors.s3.transferutility.TransferService"
           android:enabled="true" />
        
        
        <activity android:name="com.foodcuboprovider.Name"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
        <activity android:name="com.foodcuboprovider.Edit_Menu"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
        <activity android:name="com.foodcuboprovider.Discount"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
         <activity android:name="com.foodcuboprovider.Location"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
          <activity android:name="com.foodcuboprovider.Charges"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
           <activity android:name="com.foodcuboprovider.Display"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
             <activity android:name="com.foodcuboprovider.AddingMenu"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
             <activity android:name="StatusPage"
            android:label="@string/app_name"
        android:theme="@style/AppTheme" />
            
    </application>

</manifest>
